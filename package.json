    "ice.js": "^2.0.0",
    "lint-staged": "^10.5.4",
    "stylelint": "^13.1.0"
  },
  "scripts": {
    "service": "node ./service/index.js",
    "start": "icejs start --mode dev --port=8080",
    "dev": "icejs start --mode dev --port=3000",
    "build:daily": "icejs build --mode daily",
    "build": "icejs build --mode prod",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint --cache --ext .js,.jsx ./",
    "fix": "eslint --fix --ext .js,.jsx,.tsx ./src",
    "stylelint": "stylelint ./**/*.less",
    "lint-staged": "lint-staged",
    "lint-commit": "commitlint -E HUSKY_GIT_PARAMS"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ice-lab/react-materials/tree/master/scaffolds/antd-lite"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npm run lint-commit",
      "pre-commit": "npm run lint-staged"
    }
  },
  "private": true,
  "originTemplate": "@icedesign/ice-antd-scaffold"
