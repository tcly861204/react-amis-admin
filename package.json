    "amis": "1.9",
    "amis-editor": "4.1.0-beta.4",
    "antd": "^4.0.0",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "jest": "^27.4.4",
    "lodash": "^4.17.21",
    "numeral": "^2.0.6",
    "prop-types": "^15.5.8",
    "qs": "^6.10.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@iceworks/spec": "^1.0.1",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "build-plugin-antd": "^0.1.2",
    "build-plugin-ignore-style": "^0.1.0",
    "cors": "^2.8.5",
